{
  "extends": "vaadin",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "Polymer": false,
    "Predix": false,
    "Px": false,
    "PxColorsBehavior": false,
    "PxVisBehavior": false,
    "PxVisBehaviorChart": false,
    "PxVisBehaviorD3": false,
    "PxBehaviorHeatmap": false,
    "Vaadin": false
  },
  "rules": {
    "max-len": 0
  }
}
